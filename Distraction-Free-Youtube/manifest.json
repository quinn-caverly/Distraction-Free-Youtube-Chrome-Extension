{
    "name":"Distraction Free Youtube",
    "description":"Removes attention grabbing and distracting content from Youtube",
    "version":"0.0.1",

    "icons": {
        "16":"icon.png",
        "32":"icon.png",
        "128":"icon.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["blockcontent.js"]
        }
    ],
    "manifest_version":2
}
